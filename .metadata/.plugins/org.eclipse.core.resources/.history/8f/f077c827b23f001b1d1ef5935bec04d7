public class Test {

	public static void main(String[] args) {
		int
		
		//1 . 객체 생성하고 그 객체에 메소드 호출
		Test t1 = new Test();
		t1.test1();
		t1.test1();
		t1.test1();
		t1.test1();
		
		
		//2. 인스턴스  객체 메소드            --------일회용
		new Test().test2();
		
	}
	
	
	public void test1() {
		System.out.println("test1");
	}
	public void test2() {
		System.out.println("test2");
	}

}
