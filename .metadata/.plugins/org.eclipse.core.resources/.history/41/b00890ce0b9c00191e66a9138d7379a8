package _1;

public class _3 {

	//문제 주소: https://www.acmicpc.net/problem/10171
	
	public static void main(String[] args) {
		
		System.out.println("\\    /\\");
		System.out.println(")  ( ')");
		System.out.println("(  /  )");
		System.out.println("\\(__)|");
		
	}

}


//문제
//아래 예제와 같이 고양이를 출력하시오.
//
//
//	\    /\
//	)  ( ')
//	(  /  )
//	\(__)|
